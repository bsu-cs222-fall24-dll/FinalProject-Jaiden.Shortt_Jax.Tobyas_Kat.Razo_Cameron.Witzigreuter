<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="CENTER" prefHeight="400.0" prefWidth="600.0" spacing="1.0" stylesheets="@styles/bootstrap3.css" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="edu.bsu.cs.application.LeaderboardController">
      <HBox alignment="TOP_CENTER" prefHeight="31.0" prefWidth="600.0">

            <TextField prefHeight="46.0" prefWidth="230.0" />
            <Button mnemonicParsing="false" prefWidth="100.0" text="Search game">
         <styleClass>
            <String fx:value="action-btn" />
            <String fx:value="success" />
         </styleClass></Button>
      </HBox>

      <HBox alignment="TOP_CENTER" prefHeight="44.0" prefWidth="600.0">

            <ChoiceBox prefWidth="137.0" />
            <ChoiceBox prefWidth="139.0" />
            <CheckBox mnemonicParsing="false" text="Levels?" />
      </HBox>

      <TableView prefHeight="302.0" prefWidth="580.0">
        <columns>
          <TableColumn prefWidth="300.0" text="Username" />
          <TableColumn prefWidth="140.0" text="Date submitted" />
            <TableColumn prefWidth="140.0" text="Run time" />
        </columns>
         <VBox.margin>
            <Insets left="10.0" right="10.0" />
         </VBox.margin>
      </TableView>
</VBox>
